    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/three": "^0.180.0",
    "awilix": "^8.0.1",
    "lucide-react": "^0.544.0",
    "medusa-variant-images": "^1.1.4",
    "medusajs-launch-utils": "0.0.18",
    "minio": "^8.0.3",
    "pg": "^8.13.1",
